{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./resources/js",
    "lib": ["dom", "esnext"],
    "paths": {
      "@/*": ["*"],
    }
    
  },
  "include": ["resources/js/**/*", "resources/js/vite-env.d.ts"]
}